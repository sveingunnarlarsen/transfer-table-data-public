						"==OBJECT STRING==",
						"declare function apiGetTableList();\r",
						"declare function apiTransferData({data: {name}});\r",
						"declare const modelAppData: any;\r",
						"declare const oSelect: any;\r",
						"\r",
						"(async function () {\r",
						"    const tableList = await apiGetTableList();\r",
						"    modelAppData.setData({tables: tableList.dictionary, selectedTable: \"\"});     \r",
						"})();\r",
						"\r",
						"async function transferTableData() {\r",
						"    const result = await apiTransferData({data: {name: modelAppData.oData.selectedTable}});\r",
						"}\r",
						"\r",
						"console.log(\"test\");\r",
						"\r",
